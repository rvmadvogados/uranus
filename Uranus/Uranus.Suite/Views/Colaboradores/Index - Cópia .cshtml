        // Avaliações
        function ShowDadosAvaliacoes(Tipo) {
            var dados = new window.FormData();
            dados.append("IdProfissional", $("#id").val());

            var options = {};
            options.url = "/Colaboradores/CarregarDadosAvaliacao";
            options.type = "POST";
            options.data = dados;
            options.asyn = true;
            options.contentType = false;
            options.processData = false;
            options.dataType = "application/json; charset=utf-8";
            options.complete = function (data) {
                var json = $.parseJSON(data.responseText);
                if (json.response == "success") {

                    $('#DadosAvaliacoesAccordion').html(json.avaliacoes);

                    for (var i = 1; i < (json.count + 2); i++) {

                        if (Tipo == "Visualizar") {
                            $("#DataAvaliacao" + i).attr('disabled', true);
                            $("#Avaliador" + i).attr('disabled', true);
                            $("#DescricaoAvaliacao" + i).attr('disabled', true);
                            $("#DocFileAvaliacaoUpload" + i).attr('disabled', true);

                            $("#ButtonAvaliacao" + i + "Incluir").hide();
                            $("#ButtonAvaliacao" + i + "Excluir").hide();
                        }
                        else {
                            $("#DataAvaliacao" + i).attr('disabled', false);
                            $("#Avaliador" + i).attr('disabled', false);
                            $("#DescricaoAvaliacao" + i).attr('disabled', false);
                            $("#DocFileAvaliacaoUpload" + i).attr('disabled', false);

                            $("#ButtonAvaliacao" + i + "Incluir").show();
                            $("#ButtonAvaliacao" + i + "Excluir").show();
                        }
                    }
                }
            };
            $.ajax(options);
        }

        function ButtonDadoAvaliacaoSalvar_Click(index) {

            $("#colaborador-dadosavaliacoes-form").validate();

            if ($('#colaborador-dadosavaliacoes-form').valid()) {

                $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', true);
                $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', true);

                $("#ButtonAvaliacao" + index + "Incluir").html("Salvando, aguarde <i class='fa fa-spinner fa-spin'></i>");

                var dados = new window.FormData();
                dados.append("Id", $('#HiddenDadoAvaliacao' + index).val());
                dados.append("IdPessoa", $("#idPessoa").val());
                dados.append("Data", $('#DataAvaliacao' + index).val());
                dados.append("Avaliador", $('#Avaliador' + index).val());
                dados.append("Descricao", $('#DescricaoAvaliacao' + index).val());
                dados.append("DocFile", $('#DocFileAvaliacaoUpload' + index)[0].files[0]);

                var options = {};
                options.url = "/Colaboradores/SalvarDadosAvaliacao";
                options.type = "POST";
                options.data = dados;
                options.asyn = true;
                options.contentType = false;
                options.processData = false;
                options.dataType = "application/json; charset=utf-8";
                options.complete = function (data) {

                    var json = $.parseJSON(data.responseText);

                    if (json.response == "success") {
                        $("#colaborador-dadosavaliacoes-alert-success").removeClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");

                        ShowDadosAvaliacoes("Alterar");
                    }
                    else if (json.response == "warning") {
                        $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-warning").removeClass("hidden");

                        $("#ButtonAvaliacao" + index + "Incluir").text("Salvar");
                        $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', false);
                        $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', false);
                    }
                    else {
                        $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error").removeClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");

                        $("#ButtonAvaliacao" + index + "Incluir").text("Salvar");
                        $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', false);
                        $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', false);
                    }

                    setTimeout(function () {
                        $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                        $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");
                    }, 3000);
                };
                $.ajax(options);
            }
        }

        function ButtonDadoAvaliacaoExcluir_Click(index) {

            $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', true);
            $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', true);

            $("#ButtonAvaliacao" + index + "Excluir").html("Apagando, aguarde <i class='fa fa-spinner fa-spin'></i>");

            $("#ButtonAvaliacao" + index + "ExcluirConfirmacao").show();
            $("#ButtonAvaliacao" + index + "ExcluirConfirmacao button").on('click', function () {
                var confirm = $(this).attr('value');
                if (confirm == 'true') {

                    $("#ButtonAvaliacao" + index + "ExcluirConfirmacao").hide();

                    var dados = new window.FormData();
                    dados.append("Id", $('#HiddenDadoAvaliacao' + index).val());

                    var options = {};
                    options.url = "/Colaboradores/ExcluirDadosAvaliacao";
                    options.type = "POST";
                    options.data = dados;
                    options.asyn = true;
                    options.contentType = false;
                    options.processData = false;
                    options.dataType = "application/json; charset=utf-8";
                    options.complete = function (data) {

                        var json = $.parseJSON(data.responseText);

                        if (json.response == "removed") {
                            $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-removed").removeClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");

                            ShowDadosAvaliacoes("Alterar");
                        }
                        else {
                            $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error2").removeClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");

                            $("#ButtonAvaliacao" + index + "Excluir").text("Apagar");
                            $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', false);
                            $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', false);
                        }

                        setTimeout(function () {
                            $("#colaborador-dadosavaliacoes-alert-success").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-removed").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-error2").addClass("hidden");
                            $("#colaborador-dadosavaliacoes-alert-warning").addClass("hidden");
                        }, 3000);
                    };
                    $.ajax(options);

                    return false;

                } else if (confirm == 'false') {

                    $("#ButtonAvaliacao" + index + "ExcluirConfirmacao").hide();
                    $("#ButtonAvaliacao" + index + "Excluir").text("Apagar");
                    $("#ButtonAvaliacao" + index + "Incluir").attr('disabled', false);
                    $("#ButtonAvaliacao" + index + "Excluir").attr('disabled', false);

                    return false;

                }
            });

        }

