        // Evento para escutar a digitação no campo de filtro
        $('#FiltrarTipoContrato').on('input', function () {
            var filterValue = $(this).val().toLowerCase();
            // Loop através de todas as opções do select
            $('#TipoContrato option').each(function () {
                var optionText = $(this).text().toLowerCase();

                // Se o texto da opção não contiver o valor do filtro, esconde a opção
                if (optionText.indexOf(filterValue) === -1) {
                    $(this).hide();
                } else {
                    $(this).show();
                }
            });
        });
        // Evento para escutar a digitação no campo de filtro do Status (semelhante função anterior)
        $('#FiltrarStatus').on('input', function () {
            var filterValue = $(this).val().toLowerCase();
            $('#Status option').each(function () {
                var optionText = $(this).text().toLowerCase();
                if (optionText.indexOf(filterValue) === -1) {
                    $(this).hide();
                } else {
                    $(this).show();
                }
            });
        });

