                                            </div>
                                        </form>
                                        <div>
                                            @{
                                                @grid.GetHtml(
                                                        tableStyle: "webgrid",
                                                        fillEmptyRows: false,
                                                        headerStyle: "webgrid-header",
                                                        alternatingRowStyle: "webgrid-alternating-row",
                                                        rowStyle: "webgrid-row-style",
                                                        selectedRowStyle: "webgrid-selected-row",
                                                        footerStyle: "webgrid-footer",
                                                        mode: WebGridPagerModes.All,
                                                        firstText: "Primeiro",
                                                        previousText: "Anterior",
                                                        nextText: "Próximo",
                                                        lastText: "Último",
                                                        numericLinksCount: 5,
                                                        columns: new[] {
                                                        grid.Column("Pessoas.Nome", header: "Nome"),
                                                        grid.Column("Pessoas.CpfCnpj", header: "CPF/CNPJ", format: (item) => new HtmlString(item.Pessoas.CpfCnpj != null ? (item.Pessoas.CpfCnpj.Length == 11 ? Uranus.Common.Util.FormatCPF(item.Pessoas.CpfCnpj) : Uranus.Common.Util.FormatCNPJ(item.Pessoas.CpfCnpj)) : string.Empty)),
                                                        //grid.Column("Email", header: "Email", format: (item) => item.Principal),
                                                        //grid.Column("Telefone", header: "Telefone", format: (item) => item.Principal),
                                                        grid.Column("Id", header: "Ações", canSort:false,
                                                        format:
                                    @<text>
                                        @*@Html.Raw("<a class='btn btn-default btn-xs' style='background-color: green; color: white; border: green;' onclick='AbrirAPIWhatsapp('0000000000');'>WhatsApp</a>")*@
                                        <!-- @Html.Raw("<a class='btn btn-default btn-xs' onclick=''>Chat</a>") -->
                                        @Html.Raw("<a class='btn btn-default btn-xs' onclick='Alterar(" + item.Id + ", true);'>Visualizar</a>")
                                        @Html.Raw("<a class='btn btn-primary btn-xs' onclick='Alterar(" + item.Id + ", false);'>Alterar</a>")
                                        @Html.Raw("<a class='btn btn-danger  btn-xs' data-toggle='modal' data-target='#modalExcluir' onclick='Excluir(" + item.Id + ", " + '"' + item.Pessoas.Nome + '"' + ", " + item.IdPessoa + ");'>Excluir</a>")
                                    </text>)
});

                                                int firstRecord = (grid.PageIndex * grid.RowsPerPage) + 1;
                                                int lastRecord = (grid.PageIndex * grid.RowsPerPage) + grid.Rows.Count();

                                                var totalRecord = "Nenhuma linha encontrada";

                                                if (grid.TotalRowCount == 1)
                                                {
                                                    totalRecord = "Mostrando " + firstRecord + " a " + lastRecord + " de " + grid.TotalRowCount + " linha";
                                                }
                                                else if (grid.TotalRowCount > 1)
                                                {
                                                    totalRecord = "Mostrando " + firstRecord + " a " + lastRecord + " de " + grid.TotalRowCount + " linhas";
                                                }

                                                <div class="right">@totalRecord</div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
