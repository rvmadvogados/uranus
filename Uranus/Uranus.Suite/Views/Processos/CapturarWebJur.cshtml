                if ($('#WebJurPrazoEvento1').val() != "" || $('#WebJurPrazoEvento2').val() != "") {

                    var date1 = toDate($('#WebJurPrazoEvento1').val());
                    var date2 = toDate($('#WebJurPrazoEvento2').val());
                    var days = (date2 - date1) / (1000 * 60 * 60 * 24);

                    if (days < 0) {

