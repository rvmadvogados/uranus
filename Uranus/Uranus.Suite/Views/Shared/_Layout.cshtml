    <div id="myModalPass" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <form id="senha-form">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" style="font-size: 24pt;">&times;</span></button>
                        <h3 class="modal-title">Alterar Senha</h3>
                    </div>
                    <div class="modal-body">
                        <input id="username" type="hidden" class="form-control" value="@Uranus.Suite.Sessao.Usuario.Nome" />
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="titlefield">Senha <span class="required">*</span></label>
                                    <div class="input-group" id="show_hide_password1">
                                        <input type="password" class="form-control" placeholder="Informe uma Senha" id="SenhaAux" name="SenhaAux" data-msg-required="Por favor, insira uma Senha." data-rule-required="true" />
                                        <div class="input-group-addon">
                                            <a href="" style="color: #000 !important;"><i class="fa fa-eye-slash" aria-hidden="true"></i></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="titlefield">Confirmar Senha <span class="required">*</span></label>
                                    <div class="input-group" id="show_hide_password2">
                                        <input type="password" class="form-control" placeholder="Informe a mesma Senha" id="ConfirmaSenhaAux" name="ConfirmaSenhaAux" data-msg-required="Por favor, insira a Senha de Confirmação." data-rule-required="true" />
                                        <div class="input-group-addon">
                                            <a href="" style="color: #000 !important;"><i class="fa fa-eye-slash" aria-hidden="true"></i></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button id="buttonAlterarSenha" type="button" class="btn btn-primary" style="margin: 0;" data-dismiss="modal">Salvar</button>
                        &nbsp;
                        <button type="button" class="btn btn-default" style="margin: 0;" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </form>

        </div>
    </div>

    <div id="myModalInfo" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <form id="empresa-info-form" method="POST" enctype="multipart/form-data">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" style="font-size: 24pt;">&times;</span></button>
