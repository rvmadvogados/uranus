        function FormatNumber(value, pattern) {
            var i = 0,
                phone = value.toString();
            return pattern.replace(/#/g, _ => phone[i++]);
        }

        function toDate(dateStr) {
            var parts = dateStr.split("/")
            return new Date(parts[2], parts[1] - 1, parts[0])
        }

        function formatBytes(bytes, decimals, binaryUnits) {
            if (bytes == 0) {
                return '0 Bytes';
            }
            var unitMultiple = (binaryUnits) ? 1024 : 1000;
            var unitNames = (unitMultiple === 1024) ? // 1000 bytes in 1 Kilobyte (KB) or 1024 bytes for the binary version (KiB)
                ['Bytes', 'KiB', 'MiB', 'GiB', 'TiB', 'PiB', 'EiB', 'ZiB', 'YiB'] :
                ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];
            var unitChanges = Math.floor(Math.log(bytes) / Math.log(unitMultiple));
            return parseFloat((bytes / Math.pow(unitMultiple, unitChanges)).toFixed(decimals || 0)) + '' + unitNames[unitChanges];
        }

        function GetTodayDate() {

            var tdate = new Date();
            var dd = tdate.getDate(); //yields day
            var MM = tdate.getMonth(); //yields month
            var yyyy = tdate.getFullYear(); //yields year
            var currentDate = AddLeadingZeros(dd, 2) + "/" + AddLeadingZeros(MM + 1, 2) + "/" + yyyy;

            return currentDate;

        }

        function AddLeadingZeros(n, length) {
            var str = (n > 0 ? n : -n) + "";
            var zeros = "";
            for (var i = length - str.length; i > 0; i--)
                zeros += "0";
            zeros += str;
            return n >= 0 ? zeros : "-" + zeros;
        }

        function addDays(myDate, mydays) {

            var formatteddate = myDate.split("/").reverse().join("-");

            var date = new Date(formatteddate),
                days = parseInt(mydays, 10);

            if (!isNaN(date.getTime())) {
                date.setDate(date.getDate() + days);

                var dd = date.getDate();
                var MM = date.getMonth();
                var yyyy = date.getFullYear();

                var newdate = AddLeadingZeros(dd, 2) + "/" + AddLeadingZeros(MM + 1, 2) + "/" + yyyy;

                return newdate;

            } else {

                console.log('Data inválida.')

            }
        }

        function numberToReal(numero) {

            var numero = parseFloat(numero).toFixed(2).split('.');

            numero[0] = numero[0].split(/(?=(?:...)*$)/).join('.');

            return numero.join(',');

        }

