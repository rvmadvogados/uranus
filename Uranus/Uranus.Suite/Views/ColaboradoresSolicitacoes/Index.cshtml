        $("#solicitacaoespecial-form").validate().resetForm();
    }

    function AlterarSolicitacaoEspecial(Id) {
        $('#modalLabelTitle').text('Alterar Solicitaço Especial');

        $('#SolicitacaoEspecialData').attr('disabled', false);
        $('#SolicitacaoEspecialData').val('');
        $('#SolicitacaoEspecialSolicitacao').attr('disabled', false);
        $('#SolicitacaoEspecialSolicitacao').val($('').val());
        $('#SolicitacaoEspecialDataAprovacao').attr('disabled', false);
        $('#SolicitacaoEspecialDataAprovacao').val('');
        $('#SolicitacaoEspecialStatus').attr('disabled', false);
        $('#SolicitacaoEspecialStatus').val('');
        $('#SolicitacaoEspecialJustificativa').attr('disabled', false);
        $('#SolicitacaoEspecialJustificativa').val($('').val());

        $('#buttonSalvar').attr('disabled', false);
        $("#solicitacaoespecial-form").validate().resetForm();



        var dados = new window.FormData();

        dados.append("Id", Id);

        var options = {};
        options.url = "/ColaboradoresSolicitacoes/ConsultarSolicitacaoEspecial"
        options.type = "POST";
        options.data = dados;
        options.asyn = true;
        options.contentType = false;
        options.processData = false;
        options.dataType = "application/json; charset=utf-8";
        options.complete = function (data) {

            var json = $.parseJSON(data.responseText);

            if (json.codigo == '00') {

                $('#id').val(Id);
                $('#SolicitacaoEspecialData').val(json.colaboradorsolicitacaoespecial[0].Data);
                $('#SolicitacaoEspecialSolicitacao').val(json.colaboradorsolicitacaoespecial[0].Solicitacao);
                $('#SolicitacaoEspecialDataAprovacao').val(json.colaboradorsolicitacaoespecial[0].DataAprovacao);
                $('#SolicitacaoEspecialStatus').val(json.colaboradorsolicitacaoespecial[0].Status);
                $('#SolicitacaoEspecialJustificativa').val(json.colaboradorsolicitacaoespecial[0].Justificativa);

                $('#SolicitacaoEspecialDataCadastro').val(json.colaboradorsolicitacaoespecial[0].DataCadastro);
                $('#SolicitacaoEspecialUsuarioCadastro').val(json.colaboradorsolicitacaoespecial[0].UsuarioCadastro);
                $('#SolicitacaoEspecialDataAlteracao').val(json.colaboradorsolicitacaoespecial[0].DataAlteracao);
                $('#SolicitacaoEspecialUsuarioAlteracao').val(json.colaboradorsolicitacaoespecial[0].UsuarioAlteracao);

                $('#AtestadoFileName').html(json.arquivo);

            }
        };
        $.ajax(options);
    }

    $('#buttonSalvarSolicitacaoEspecial').click(function (evt) {

        $("#ausencia-form").validate();

        if ($('#ausencia-form').valid()) {

            var dados = new window.FormData();

            dados.append("Id", $("#id").val());
            dados.append("IdProfissional", $("#idProfissional").val());
            dados.append("Data", $("#SolicitacaoEspecialData").val());
            dados.append("Solicitacao", $("#SolicitacaoEspecialSolicitacao").val());
            dados.append("DataAprovacao", "");
            dados.append("Status", $("#SolicitacaoEspecialStatus").val());
            dados.append("Justificativa", $("#SolicitacaoEspecialJustificativa").val());

            var options = {};
            options.url = "/ColaboradoresSolicitacoes/SalvarSolicitacaoEspecial"
            options.type = "POST";
            options.data = dados;
            options.asyn = true;
            options.contentType = false;
            options.processData = false;
            options.dataType = "application/json; charset=utf-8";
            options.complete = function (data) {

                var json = $.parseJSON(data.responseText);

                if (json.codigo == '00') {

                    $('h4').text(json.mensagem);
                    $('#modalMensagem').attr('data-backdrop', 'static');
                    $('#modalMensagem').attr('data-keyboard', 'false');
                    $('#modalMensagem').modal('show');
                    $("#solicitacaoferias-form").validate().resetForm();
                    Atualizar();
                }
                else {
                    $('h4').text(json.mensagem);
                    $('#modalMensagem').attr('data-backdrop', 'static');
                    $('#modalMensagem').attr('data-keyboard', 'false');
                    $('#modalMensagem').modal('show');
                    $("#solicitacaoferias-form").validate().resetForm();
                }
            };
            $.ajax(options);

            evt.preventDefault();
        }
        else {
            return false;
        }
    });

    function ExcluirSolicitacaoEspecial(Id) {
        $('h4').text('Você deseja realmente excluir essa Solicitação Especial(' + Id + ')?');
        $('#id').val(Id);
