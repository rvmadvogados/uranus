                                    <label class="titlefield">Tipo Documento</label>
                                    <select id="DocumentosTipoDocumento" name="DocumentosTipoDocumento" class="form-control placeholder">
                                        <option value="" style="background-color: #eaeaea;">Selecione o Tipo</option>
                                        <option value="1">Aviso de Férias</option>
                                        <option value="2">Carteira OAB</option>
                                        <option value="3">Carteira Profissional</option>
                                        <option value="4">Certidão Nascimento</option>
                                        <option value="5">CPF</option>
                                        <option value="6">Comprovante de Residência</option>
                                        <option value="7">Informe de Rendimentos</option>
                                        <option value="8">RG</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="titlefield">Número Documento</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="DocumentosNumeroDocumento" name="DocumentosNumeroDocumento" />
                                </div>
                            </div>
