
@{
    ViewBag.Title = "Solicitações Colaborador";
}
<div class="main_container">
    <input type="hidden" id="id" name="id" value="0">
    <input type="hidden" id="idContrato" name="idContrato" value="0">
    <input type="hidden" id="idAusencia" name="idAusencia" value="0">
    <input type="hidden" id="idSolicitacaoEspecial" name="idSolicitacaoEspecial" value="0">
    <input type="hidden" id="idProfissional" name="idProfissional" value="@Model.Profissional.Id">
    <input type="hidden" id="nome" name="nome" value="@Model.Profissional.Nome">
    <input type="hidden" id="saldoPeriodo" name="saldoPeriodo" value="@Model.Profissional.SaldoPeriodo">
    <input type="hidden" id="idPeriodoAquisitivo" name="idPeriodoAquisitivo" value="@Model.Profissional.IdPeriodoAquisitivo">

    <style>
        div.upload {
            background-color: #fff;
            border: 1px solid #ccc;
            display: inline-block;
            height: 38px;
            padding: 3px 40px 3px 3px;
            position: relative;
            width: 100% !important;
        }

            div.upload:hover {
                opacity: 0.95;
            }

            div.upload input[type="file"] {
                display: input-block;
                width: 100%;
                height: 30px;
                opacity: 0;
                cursor: pointer;
                position: absolute;
                left: 0;
            }

        .uploadButton {
            background-color: #333333;
            border: none;
            color: #FFF;
            cursor: pointer;
            display: inline-block;
            height: 30px;
            margin-right: 15px;
            width: auto;
            padding: 0 20px;
            box-sizing: content-box;
        }

            .uploadButton:hover {
                background-color: #3D3D3D;
            }

        .fileName {
            font-family: Arial;
            font-size: 14px;
        }

        .upload + .uploadButton {
            height: 40px;
        }
    </style>

    <div id="modalNovaSolicitacaoFerias" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <form id="solicitacaoferias-form">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" style="font-size: 24pt;">&times;</span></button>

                        @*<h3 class="modal-title">Incluir Solicitação de Recesso</h3>*@
                        @{
                            if (Model.Profissional.Tipo == "Advogado" || Model.Profissional.Tipo == "Advogado Sócio")
                            {
                                <h3 class="modal-title">Incluir Solicitação de Recesso</h3>
                            }
                            else
                            {
                                <h3 class="modal-title">Incluir Solicitação de Férias</h3>
                            }
                        }
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Data Solicitação</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasDataSolicitacao" name="SolicitcaoFeriasDataSolicitacao" readonly />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Data Aprovação</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasDataAprovacao" name="SolicitcaoFeriasDataAprovacao" readonly />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Status</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasStatus" name="SolicitcaoFeriasStatus" readonly />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Saldo</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasSaldo" name="SolicitcaoFeriasSaldo" readonly />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Data Início</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasDataInicio" name="SolicitcaoFeriasDataInicio" readonly />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="titlefield">Dias</label>
                                    <input type="text" style="height: 40px !important; cursor: pointer;" class="form-control" id="SolicitcaoFeriasDias" name="SolicitcaoFeriasDias" />
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 hidden" id="Especial">
                                <label class="titlefield">Solicitação Especial </label>
                                <br />
                                <div class="radio-item">
                                    <input type="radio" id="SolicitcaoFeriasEspecialSim" name="SolicitcaoFeriasEspecial" value="S">
                                    <label for="SolicitcaoFeriasEspecialSim">Sim</label>
                                </div>
                                <div class="radio-item">
                                    <input type="radio" id="SolicitcaoFeriasEspecialNao" name="SolicitcaoFeriasEspecial" value="N">
                                    <label for="SolicitcaoFeriasEspecialNao">Não</label>
                                </div>
                                @*<label id="SolicitcaoFeriasEspecialValidate" name="SolicitcaoFeriasEspecialValidate" for="SolicitcaoFeriasEspecial" class="error hidden"> Dias Solicitados maior que o Saldo - Para concluir esta solicitação tem que selecionar Solicitação Especial</label>*@
                                <br />
                                <label id="SolicitcaoFeriasEspecialValidate" class="hidden" style="color:red">
                                    Dias solicitados maior que o saldo - Selecione "Solicitação Especial"
                                </label>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="titlefield">Justificativa</label>
                                    <textarea id="SolicitcaoFeriasJustificativa" name="SolicitcaoFeriasJustificativa" class="form-control placeholder" placeholder="Informe aqui a Justificativa"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="titlefield">Data Cadastro</label>
                                    <input type="text" class="form-control" id="SolicitcaoFeriasDataCadastro" name="SolicitcaoFeriasDataCadastro" readonly />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="titlefield">Usuário Cadastro</label>
                                    <input type="text" class="form-control" id="SolicitcaoFeriasUsuarioCadastro" name="SolicitcaoFeriasUsuarioCadastro" readonly />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="titlefield">Data Alteração</label>
                                    <input type="text" class="form-control" id="SolicitcaoFeriasDataAlteracao" name="SolicitcaoFeriasDataAlteracao" readonly />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="titlefield">Usuário Alteração</label>
                                    <input type="text" class="form-control" id="SolicitcaoFeriasUsuarioAlteracao" name="SolicitcaoFeriasUsuarioAlteracao" readonly />
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <button id="buttonSalvarSolicitacoesFerias" type="button" class="btn btn-primary" style="margin: 0;" data-dismiss="modal">Salvar</button>
                        &nbsp;
                        <button type="button" class="btn btn-default" style="margin: 0;" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </form>

        </div>
    </div>

    <div id="modalExcluirSolicitacaoFerias" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" style="font-size: 24pt;">&times;</span></button>
                    <h3 class="modal-title" id="modalLabelExcluir">Excluir Solicitação</h3>
                </div>
                <div class="modal-body">
                    <h4>Você deseja realmente excluir essa Solicitação?</h4>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" style="margin: 0;" data-dismiss="modal" onclick="ExcluirConfirmadoSolicitacao();">Sim</button>
                    &nbsp;
                    <button type="button" class="btn btn-default" style="margin: 0;" data-dismiss="modal">Não</button>
                </div>
            </div>

        </div>
    </div>

    <div id="modalNovoDocumento" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <form id="documento-form">
