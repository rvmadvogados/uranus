    function DownloadContracheque(Id) {

        var dados = new window.FormData();
        dados.append("Id", Id);

        var options = {};
        options.url = "/ColaboradoresSolicitacoes/DownloadContracheque"
        options.type = "POST";
        options.data = dados;
        options.asyn = true;
        options.contentType = false;
        options.processData = false;
        options.dataType = "application/json; charset=utf-8";
        options.complete = function (data) {

            var json = $.parseJSON(data.responseText);

            if (json.codigo == '00') {


                const linkSource = `data:application/pdf;base64,${json.documento}`;
                const link = document.createElement("a");
                const fileName = json.nomearquivo;

                link.href = linkSource;
                link.download = fileName;
                link.click();

            }

        };
        $.ajax(options);
    }

