    function ExcluirConfirmadoSolicitacaoEspecial() {
        var dados = new window.FormData();
        dados.append("Id", $("#id").val());

        $('#id').val('');

        var options = {};
        options.url = "/ColaboradoresSolicitacoes/ExcluirSolicitacaoEspecial"
