<script>

    $(document).ready(function () {
        // Restaura a aba ativa do localStorage ao recarregar a página
        var activeTab = localStorage.getItem('activeTab');
        if (activeTab) {
            $(`a[href="${activeTab}"]`).tab('show'); // Mostra a aba salva
        } else {
            // Se não houver aba salva, mantém a aba padrão ativa
            $('#myTab a:first').tab('show');
        }

        // Salva a aba ativa no localStorage quando o usuário clica em qualquer aba
        $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            var currentTab = $(e.target).attr('href'); // Pega o href da aba ativa
            localStorage.setItem('activeTab', currentTab); // Salva no localStorage
        });

    //    // Evento de clique específico para a aba Solicitação Férias/Recesso
    //    $('#tab-solicitacaoferias').on('click', function () {
    //        Atualizar(); // Chama a função Atualizar ao clicar
    //    });
    });


    $("#SolicitcaoFeriasDataInicio").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#DocumentosDataEmissao").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#DocumentosDataValidade").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#AusenciaDataInicio").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#AusenciaDataFim").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#SolicitacaoEspeciaDataAprovacao").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $("#SolicitacaoEspeciaData").datepicker({
        language: 'pt-BR',
        autoclose: true,
        todayHighlight: true,
        orientation: "bottom",
        clearBtn: true
    });

    $('#DocumentoFileUpload').change(function (e) {
        $in = $(this);
        $in.next().html($in.val());
    });

    $('#AtestadoFileUpload').change(function (e) {
        $in = $(this);
        $in.next().html($in.val());
    });

    $('#SolicitcaoFeriasDias').on('keyup', function () {
        var isEspecial = $('input[name=SolicitcaoFeriasEspecial][value="S"]').is(':checked');

        if (isEspecial == false) {

//            var _Saldo = $('#SolicitcaoFeriasSaldo').val();
            var _Saldo = parseInt($('#SolicitcaoFeriasSaldo').val(), 10) || 0
//            var _dias = $(this).val();
            var _dias = parseInt($(this).val(), 10) || 0;
            if (_dias > _Saldo) {
                $('input[name=SolicitcaoFeriasEspecial][value="S"]').attr('disabled', false);
                $('input[name=SolicitcaoFeriasEspecial][value="N"]').attr('disabled', false);
                $('input[name=SolicitcaoFeriasEspecial][value="N"]').prop('checked', true).trigger('change');
                $('#Especial').removeClass("hidden");
                $('#SolicitcaoFeriasEspecialValidate').removeClass("hidden")
            }
            else {
                $('#Especial').addClass('hidden');
                $('#SolicitcaoFeriasEspecialValidate').addClass('hidden');

                $('input[name=SolicitcaoFeriasEspecial]').prop('checked', false);

            }

        }
    });

    function NovaSolicitacaoFerias() {

        $('#modalLabelTitle').text('Nova Solicitação Férias');
        $('#SolicitcaoFeriasDataSolicitacao').attr('disabled', false);
        $('#SolicitcaoFeriasDataSolicitacao').val('@DateTime.Now.ToString("dd/MM/yyyy")');
        $('#SolicitcaoFeriasDataAprovacao').attr('disabled', false);
        $('#SolicitcaoFeriasDataAprovacao').val('');
        $('#SolicitcaoFeriasStatus').attr('disabled', false);
        $('#SolicitcaoFeriasStatus').val('Em Análise');
        $('#SolicitcaoFeriasSaldo').attr('disabled', false);
        $('#SolicitcaoFeriasSaldo').val($('#saldoPeriodo').val());
        $('#SolicitcaoFeriasDataInicio').attr('disabled', false);
        $('#SolicitcaoFeriasDataInicio').val('');
        $('#SolicitcaoFeriasDias').attr('disabled', false);
        $('#SolicitcaoFeriasDias').val('');
        $('#SolicitcaoFeriasJustificativa').attr('disabled', false);
        $('#SolicitcaoFeriasJustificativa').val('');
        $('#SolicitcaoFeriasDataCadastro').val('');
        $('#SolicitcaoFeriasUsuarioCadastro').val('');
        $('#SolicitcaoFeriasDataAlteracao').val('');
        $('#SolicitcaoFeriasUsuarioAlteracao').val('');
        $('#id').val(0);
        $('#idContrato').val(0);
        $("#solicitacaoferias-form").validate().resetForm();
    }

    function AlterarSolicitacaoFerias(Id) {

        $('#modalLabelTitle').text('Alterar Solicitacao Ferias');
        $('#SolicitcaoFeriasDataSolicitacao').attr('disabled', false);
        $('#SolicitcaoFeriasDataSolicitacao').val('');
        $('#SolicitcaoFeriasDataAprovacao').attr('disabled', false);
        $('#SolicitcaoFeriasDataAprovacao').val('');
        $('#SolicitcaoFeriasStatus').attr('disabled', false);
        $('#SolicitcaoFeriasStatus').val('Em Análise');
        $('#SolicitcaoFeriasSaldo').attr('disabled', false);
        $('#SolicitcaoFeriasSaldo').val('');
        $('#SolicitcaoFeriasDataInicio').attr('disabled', false);
        $('#SolicitcaoFeriasDataInicio').val('');
        $('#SolicitcaoFeriasDias').attr('disabled', false);
        $('#SolicitcaoFeriasDias').val('');
        $('#SolicitcaoFeriasJustificativa').attr('disabled', false);
        $('#SolicitcaoFeriasJustificativa').val('');
        $('#SolicitcaoFeriasDataCadastro').val('');
        $('#SolicitcaoFeriasUsuarioCadastro').val('');
        $('#SolicitcaoFeriasDataAlteracao').val('');
        $('#SolicitcaoFeriasUsuarioAlteracao').val('');
        $('#buttonSalvar').attr('disabled', false);
        $("#solicitacaoferias-form").validate().resetForm();



        var dados = new window.FormData();

        dados.append("Id", Id);

        var options = {};
        options.url = "/ColaboradoresSolicitacoes/Consultar"
        options.type = "POST";
        options.data = dados;
        options.asyn = true;
        options.contentType = false;
        options.processData = false;
        options.dataType = "application/json; charset=utf-8";
        options.complete = function (data) {

            var json = $.parseJSON(data.responseText);

            if (json.codigo == '00') {

                $('#id').val(Id);
