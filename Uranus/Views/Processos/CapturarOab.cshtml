                var date1 = toDate($('#OabPrazoEvento1').val());
                var date2 = toDate($('#OabPrazoEvento2').val());
                var days = (date2 - date1) / (1000 * 60 * 60 * 24);

                if (days < 0) {

                    $('#OabDataPrazoEvento2Validate').removeClass('hidden').css("display", "");
